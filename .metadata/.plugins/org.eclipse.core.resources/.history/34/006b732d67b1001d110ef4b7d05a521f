 * 
 * 
 * private String message; private boolean success; private HttpStatus status;
 * public String getMessage() { return message; } public void setMessage(String
 * message) { this.message = message; }
 * 
 * @Override public String toString() { return "ApiResponse [message=" + message
 * + ", success=" + success + ", status=" + status + "]"; } public boolean
 * isSuccess() { return success; } public void setSuccess(boolean success) {
 * this.success = success; } public HttpStatus getStatus() { return status; }
 * public void setStatus(HttpStatus status) { this.status = status; } public
